#include<stdio.h>

int main(){
    int arr[6]={1,4,5,6,7,8};
    int min=arr[0];
    int max=arr[0];
    
    int s=sizeof(arr)/sizeof(arr[3]);
    
    for(int i=1;i<s;i++){
        if(arr[i]>max){
            max=arr[i];
        }
        if(arr[i]<min){
            min=arr[i];
        }
        
    }
    printf("%d %d" , max,min);
    
    return 0;
}
